{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.4.0",
      "destination": "wwwroot/lib/bootstrap",
      "files": [ "css/bootstrap.min.css", "js/bootstrap.min.js" ]
    },
    {
      "library": "jquery@3.5.0",
      "destination": "wwwroot/lib/jquery",
      "files": [ "jquery.min.js" ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation",
      "files": [ "jquery.validate.unobtrusive.min.js" ]
    },

    {
      "library": "jquery-validate@1.19.1",
      "destination": "wwwroot/lib/jquery-validate",
      "files": [ "jquery.validate.min.js", "localization/messages_fr.min.js" ]
    },
    {
      "library": "font-awesome@5.13.0",
      "destination": "wwwroot/lib/font-awesome",
      "files": [ "js/all.min.js" ]
    }
  ]
}